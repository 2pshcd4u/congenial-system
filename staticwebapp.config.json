{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["./images/*.{png,jpg,gif}", "./css/*"]
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; https:",
    "Strict-Transport-Security": "max-age=500; includeSubDomains; preload"
  },
  "platform": {
    "apiRuntime": "node:16"
  }
}
    