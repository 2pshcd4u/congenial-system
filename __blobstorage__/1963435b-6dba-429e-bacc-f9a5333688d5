/**
 * This method returns `undefined`.
 *
 * @static
 * @memberOf _
 * @since 2.3.0
 * @category Util
 * @example
 *
 * _.times(2, _.noop);
 * // => [undefined, undefined]
 */
function noop() {
  // No operation performed.
}

module.exports = noop;
'use strict';

module.exports = require('./async').doUntil;
{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"max-nested-callbacks": [2, 3]
	},

	"overrides": [
		{
			"files": "test/**",
			"rules": {
				"prefer-regex-literals": 0,
			},
		},
