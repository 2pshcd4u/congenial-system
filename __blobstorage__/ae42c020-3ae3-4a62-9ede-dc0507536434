  "description": "Escape RegExp special characters",
  "license": "MIT",
  "repository": "sindresorhus/escape-string-regexp",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
