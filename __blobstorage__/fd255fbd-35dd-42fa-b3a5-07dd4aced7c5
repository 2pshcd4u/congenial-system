  var loc = {
    start: start,
    end: end
  };
  n.loc = loc;
  return n;
}
export function withRaw(n, raw) {
  n.raw = raw;
  return n;
}
export function funcParam(valtype, id) {
  return {
    id: id,
    valtype: valtype
  };
}
export function indexLiteral(value) {
  // $FlowIgnore
  var x = numberLiteralFromRaw(value, "u32");
  return x;
}
export function memIndexLiteral(value) {
  // $FlowIgnore
  var x = numberLiteralFromRaw(value, "u32");
  return x;
